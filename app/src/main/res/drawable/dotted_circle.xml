<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape android:shape="oval">
            <solid android:color="@color/transperent" />
            <stroke
                android:dashGap="5dp"
                android:dashWidth="5dp"
                android:width="2dp"
                android:color="#acacac" />
            <padding
                android:bottom="5dp"
                android:left="5dp"
                android:right="5dp"
                android:top="5dp" />
        </shape>
    </item>
</layer-list>
